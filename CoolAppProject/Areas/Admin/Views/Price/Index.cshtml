<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/ea89281bc3.js" crossorigin="anonymous"></script>
</head>
<body>

    @model IEnumerable<CooursePrice>

    <div class="table-responsive">
        <table class="table">
            <thead class="table-dark">
                <tr>
                    <th scope="col">FromPrice</th>
                    <th scope="col">ToPrice</th>
                    <th scope="col">Delete</th>
                    <th scope="col">Update</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.FromPrice</td>
                        <td>@item.ToPrice</td>
                        <td><a asp-controller="price" asp-action="delete" asp-route-id="@item.Id">Delete</a></td>
                        <td><a asp-controller="price" asp-action="update" asp-route-id="@item.Id">Update</a></td>
                    </tr>
                }
            </tbody>
        </table>
    <a class="btn btn-primary" asp-controller="price" asp-action="create" role="button">Qiymet Əlavı Et</a>
    </div>
    </div>

</body>
</html>
